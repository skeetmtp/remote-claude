{
  "name": "claude-proxy",
  "version": "1.0.0",
  "description": "CLI proxy for the claude CLI that preserves full interactive terminal behavior",
  "main": "dist/index.js",
  "scripts": {
    "start": "DEBUG=proxy:* tsx src/index.ts",
    "build": "tsc",
    "dev": "tsx watch src/index.ts"
  },
  "keywords": [
    "claude",
    "proxy",
    "cli",
    "pty"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "debug": "^4.3.4",
    "eventsource": "^2.0.2",
    "node-pty": "^1.1.0",
    "qrcode-terminal": "^0.12.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/eventsource": "^1.1.15",
    "@types/node": "^20.10.0",
    "@types/qrcode-terminal": "^0.12.2",
    "@types/uuid": "^9.0.7",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
